							"  const expectedAuthors = [\"Ichiro Kishimi\", \"Fumitake Koga\"];\r",
							"  const expectedCategories = [\"Self-Help, Relationships & Lifestyle - Psychological Self-Help\"];\r",
							"  const expectedYear = 2019;\r",
							"  const expectedPublisher = \"Gramedia pustaka utama\";\r",
							"  const expectedPages = 344;\r",
							"\r",
							"  pm.expect(book).to.have.ownProperty('id');\r",
							"  pm.expect(book.id).to.equals(expectedId);\r",
							"\r",
							"  pm.expect(book).to.have.ownProperty('title');\r",
							"  pm.expect(book.title).to.equals(expectedTitle);\r",
							"\r",
							"  pm.expect(book).to.have.ownProperty('authors');\r",
							"  pm.expect(book.authors).to.eql(expectedAuthors);\r",
							"\r",
							"  pm.expect(book).to.have.ownProperty('categories');\r",
							"  pm.expect(book.categories).to.eql(expectedCategories);\r",
							"\r",
							"  pm.expect(book).to.have.ownProperty('year');\r",
							"  pm.expect(book.year).to.equals(expectedYear);\r",
							"\r",
							"  pm.expect(book).to.have.ownProperty('publisher');\r",
							"  pm.expect(book.publisher).to.equals(expectedPublisher);\r",
							"\r",
							"  pm.expect(book).to.have.ownProperty('pages');\r",
							"  pm.expect(book.pages).to.equals(expectedPages);\r",
							"});"
